{
  "name": "accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "injectOptionsFromRemoteContext": true,
    "validateUpsert": true
  },
  "properties": {
    "fullname": {
      "type": "string",
      "default": ""
    },
    "avatar_url": {
      "type": "string",
      "default": ""
    },
    "gender": {
      "type": "string",
      "default": ""
    },
    "biography": {
      "type": "string",
      "default": ""
    },
    "website": {
      "type": "string",
      "default": ""
    },
    "service_count": {
      "type": "number",
      "default": 0
    },
    "expert_title": {
      "type": "string",
      "default": " "
    }
  },
  "validations": [],
  "relations": {
    "categories": {
      "type": "hasMany",
      "model": "categories",
      "foreignKey": "account_id"
    },
    "bookings": {
      "type": "hasMany",
      "model": "bookings",
      "foreignKey": "account_id"
    },
    "ratings": {
      "type": "hasMany",
      "model": "ratings",
      "foreignKey": "account_id"
    },
    "locations": {
      "type": "hasOne",
      "model": "locations",
      "foreignKey": "account_id"
    },
    "comments": {
      "type": "hasMany",
      "model": "comments",
      "foreignKey": "owner_id"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
