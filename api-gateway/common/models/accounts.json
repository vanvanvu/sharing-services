{
  "name": "accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "injectOptionsFromRemoteContext": true,
    "validateUpsert": true
  },
  "properties": {
    "fullname": {
      "type": "string",
      "default": ""
    },
    "avatarUrl": {
      "type": "string",
      "default": ""
    },
    "isFemale": {
      "type": "boolean",
      "default": false
    },
    "isExpert": {
      "type": "boolean",
      "default": false
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "serviceCount": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "categories": {
      "type": "hasMany",
      "model": "categories",
      "foreignKey": "accountId"
    },
    "bookings": {
      "type": "hasMany",
      "model": "bookings",
      "foreignKey": "accountId"
    },
    "ratings": {
      "type": "hasMany",
      "model": "ratings",
      "foreignKey": "accountId"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
