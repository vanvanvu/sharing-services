{
  "name": "accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "injectOptionsFromRemoteContext": true,
    "validateUpsert": true
  },
  "properties": {
    "fullname": {
      "type": "string",
      "default": ""
    },
    "avatar_url": {
      "type": "string",
      "default": ""
    },
    "gender": {
      "type": "string",
      "default": ""
    },
    "biography": {
      "type": "string",
      "default": ""
    },
    "website": {
      "type": "string",
      "default": ""
    },
    "expert_title": {
      "type": "string",
      "default": ""
    },
    "descs": {
      "type": "string",
      "default": ""
    },
    "price": {
      "type": "string",
      "default": ""
    },
    "level": {
      "type": "string",
      "default": " "
    },
    "rating": {
      "type": "number",
      "default": 0
    },
    "geo_name": {
      "type": "string",
      "default": " "
    },
    "geo_country": {
      "type": "string",
      "default": ""
    },
    "geo_latitude": {
      "type": "string",
      "default": " "
    },
    "geo_longtitude": {
      "type": "string",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "categories": {
      "type": "hasMany",
      "model": "categories",
      "foreignKey": "account_id"
    },
    "bookings": {
      "type": "hasMany",
      "model": "bookings",
      "foreignKey": "account_id"
    },
    "comments": {
      "type": "hasMany",
      "model": "comments",
      "foreignKey": "account_id"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "__create__comments"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__create__comments"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "__updateById__comments"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "__delete__comments"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__updateById__comments"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__delete__comments"
    }
  ],
  "methods": {}
}
