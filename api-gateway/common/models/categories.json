    "validateUpsert": true
  },
  "properties": {
    "servicename": {
      "type": "string"
    },
    "brief": {
      "type": "string"
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string"
    }
