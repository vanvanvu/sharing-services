{
  "name": "categories",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "servicename": {
      "type": "string",
      "default": ""
    },
    "brief": {
      "type": "string",
      "default": ""
    },
    "thumbnailUrl": {
      "type": "string",
      "default": "link-to-image"
    },
    "imageUrl": {
      "type": "string",
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "accounts": {
      "type": "belongsTo",
      "model": "accounts",
      "foreignKey": "account_id"
    },
    "comments": {
      "type": "hasMany",
      "model": "comments",
      "foreignKey": "service_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
